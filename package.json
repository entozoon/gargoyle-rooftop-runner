{
  "name": "gargoyle-runner",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://entozoon.github.io/gargoyle-runner",
  "dependencies": {
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "phaser-ce": "^2.9.1",
    "pixi.js": "^4.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.8.6",
    "react-scripts": "1.0.16",
    "remove": "^0.1.5"
  },
  "scripts": {
    "start": "npm-run-all --parallel react-start compile-css",
    "kanban": "readme-kanban-board",
    "react-start": "react-scripts start",
    "prebuild": "del-cli docs && npm run kanban",
    "build": "react-scripts build && ren build docs",
    "deploy": "echo no need, build and push to master (/docs)",
    "compile-css": "node-sass src --output src/ --output-style compressed --watch",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "del-cli": "^1.1.0",
    "gh-pages": "^1.1.0",
    "readme-kanban-board": "^1.4.1"
  }
}
